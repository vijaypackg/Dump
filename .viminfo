	      src: index.j2
	      dest: /var/www/html/index.html
	     notify: restartwebserver
	
	  handlers:
	   - name: startwebserver
	     service:
	      name: apache2
	      state: started
	
	   - name: restartwebserver
	     service:
	      name: apache2
	      state: restarted
	
