	---
	- hosts: all
	  become: yes
	  tasks:
	   - include_tasks: myfile.yml
	   - name: this is to install webserver
	     apt:
	      name: apache2
	      state: present
	     notify: startwebserver
	
	   - name: this is to restart the webserver by copying template
